#!/bin/bash

make

export apm_executable=./apm_parallel
export data_dir=./dna


echo "Running WORKFLOW TEST"
OMP_NUM_THREADS=12 salloc -N 2 -n 2 mpirun $apm_executable 0 $data_dir/small_chrY_x100.fa $(cat $data_dir/line_10.fa)
echo " "

echo "Running WORKFLOW TEST"
OMP_NUM_THREADS=1 salloc -N 2 -n 6 mpirun $apm_executable 0 $data_dir/small_chrY_x100.fa $(cat $data_dir/line_10.fa)
echo " "

#!/bin/bash

make

export apm_executable=./apm_parallel
export data_dir=./dna


echo "Running PATTERNS_OVER_RANKS"
OMP_NUM_THREADS=1 salloc -N 6 -n 12 mpirun $apm_executable 0 $data_dir/small_chrY_medium.fa $(cat $data_dir/line_10.fa) "PATTERNS_OVER_RANKS"
echo " "

echo "Running DB_OVER_RANKS"
OMP_NUM_THREADS=1 salloc -N 6 -n 12 mpirun $apm_executable 0 $data_dir/small_chrY_medium.fa $(cat $data_dir/line_1131.fa) "DB_OVER_RANKS"
echo " "
